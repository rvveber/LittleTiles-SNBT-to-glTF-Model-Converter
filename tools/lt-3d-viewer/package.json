{
  "name": "lt-3d-viewer",
  "version": "0.1.0",
  "private": true,
  "description": "LittleTiles-focused 3D viewer bootstrap based on Khronos glTF-Sample-Viewer",
  "type": "module",
  "scripts": {
    "prepare:renderer": "npm --prefix ../../subrepos/glTF-Sample-Viewer/glTF-Sample-Renderer install && npm --prefix ../../subrepos/glTF-Sample-Viewer/glTF-Sample-Renderer run build",
    "build:ui": "rollup -c",
    "build": "npm run prepare:renderer && npm run build:ui",
    "dev:watch": "rollup -c -w",
    "dev:serve": "http-server ./dist -p 8001 -c-1",
    "dev": "npm run prepare:renderer && npm run build:ui && concurrently \"npm run dev:watch\" \"npm run dev:serve\""
  },
  "dependencies": {
    "@khronosgroup/gltf-viewer": "file:../../subrepos/glTF-Sample-Viewer/glTF-Sample-Renderer",
    "@ntohq/buefy-next": "^0.1.3",
    "bulma": "^0.9.4",
    "fast-png": "^6.2.0",
    "gl-matrix": "^3.2.1",
    "gltf-validator": "^2.0.0-dev.3.10",
    "jpeg-js": "^0.4.3",
    "normalize-wheel": "^1.0.1",
    "path": "^0.12.7",
    "rxjs": "^6.6.7",
    "simple-dropzone": "^0.8.0",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-wasm": "^6.2.2",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "rollup": "^4.23.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-glslify": "^1.3.1",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.77.5"
  }
}
